docker build \
    --build-arg EXPOSE_PORT="80 443" \
    -t 10.5.24.46:80/nscloud/mun:1.0.0.1 .

docker run -it -d  --privileged=true -p 80:80 -p 443:443 \
    -e API_domain="nscloud.api.nsfocus.com" \
    -e Portal_domain="www.nscloud.com" \
    -e API_Proxy_IP="10.5.24.33" \
    -e pub_Redis="10.5.24.60" \
    -e pub_Redis_PWD="123456" \
    -e Atsani_Redis="10.5.24.60" \
    -e Atsani_Redis_PWD="123456" \
    10.5.24.46:80/nscloud/mun:1.0.0.1

